<?xml version="1.0"?>
<object name="portal_skins" 
        meta_type="Plone Skins Tool"
        allow_any="False"
        cookie_persistence="False"
        request_varname="plone_skin">
   
    <object 
        name="aaolm_customviews"
        meta_type="Filesystem Directory View"
        directory="aaolm.customviews:skins/customviews"
        remove="True"
        />

    <skin-path name="aaolm_customviews"
               remove="True">
        <layer 
            name="aaolm_customviews"
            insert-after="custom"
            />
            
    </skin-path>
 
</object>
